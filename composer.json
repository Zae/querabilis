{
  "name": "initx/querabilis",
  "description": "Simple PHP Queue Library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "initx",
      "email": "solver89@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Initx\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Initx\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2",
    "jms/serializer": "^3.0"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit -v"
    ],
    "test-coverage": [
      "vendor/bin/phpunit -v --coverage-text --coverage-clover=build/coverage.xml"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "fzaninotto/faker": "^1.8",
    "ramsey/uuid": "^3.8"
  }
}
