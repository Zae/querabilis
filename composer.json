{
  "name": "initx/querabilis",
  "description": "Simple PHP Queue Library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "initx",
      "email": "solver89@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Initx\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Initx\\Tests\\Unit\\": "tests/unit/",
      "Initx\\Tests\\Integration\\": "tests/integration/"
    },
    "classmap": [
      "tests/_support"
    ]
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2",
    "jms/serializer": "^3.0",
    "symfony/yaml": "^4.0",
    "predis/predis": "^1.1"
  },
  "scripts": {
    "test": [
      "vendor/bin/codecept run"
    ],
    "test-coverage": [
      "vendor/bin/codecept run --coverage-text --coverage-html --coverage-xml=coverage.xml"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "fzaninotto/faker": "^1.8",
    "ramsey/uuid": "^3.8",
    "codeception/codeception": "^3.0"
  }
}
